                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>RESERVED</ipxact:name>
                        <ipxact:bitOffset>5</ipxact:bitOffset>
                        <ipxact:bitWidth>27</ipxact:bitWidth>
